{
    "ecmaVersion": 6,
    "libs": [
        "browser",
        "underscore",
        "jquery",
        "chai"
    ],
    "loadEagerly": [
        "path/to/your/js/**/*.js"
    ],
    "dontLoad": [
        "node_modules/**",
        "path/to/your/js/**/*.js"
    ],
    "plugins": {
        "modules": {},
        "es_modules": {},
        "node": {},
        "doc_comment": {
            "fullDocs": true,
            "strong": true
        }
    }
}
